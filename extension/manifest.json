{
    "name": "__MSG_name__",
    "description": "__MSG_description__",
    "version": "1.0",
    "manifest_version": 2,
    "default_locale": "en",
    "background": {
        "scripts": [
            "mime-metadata.js",
            "prefs.js",
            "dialogmanager.js",
            "content-handlers-firefox.js",
            "background.js"
        ]
    },
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "options_ui": {
        "page": "options.html"
    },
    "permissions": [
        "history",
        "tabs",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
