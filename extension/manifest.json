{
    "name": "__MSG_name__",
    "description": "__MSG_description__",
    "version": "2.10",
    "manifest_version": 2,
    "default_locale": "en",
    "applications": {
        "gecko": {
            "id": "openinbrowser@www.spasche.net",
            "strict_min_version": "57.0a1"
        }
    },
    "background": {
        "scripts": [
            "mime-metadata.js",
            "prefs.js",
            "dialogmanager.js",
            "content-disposition.js",
            "content-handlers-firefox.js",
            "background.js"
        ]
    },
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "options_ui": {
        "page": "options.html"
    },
    "permissions": [
        "history",
        "menus",
        "sessions",
        "storage",
        "tabs",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
